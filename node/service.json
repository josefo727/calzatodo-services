{
  "memory": 256,
  "ttl": 10,
  "timeout": 30,
  "minReplicas": 2,
  "maxReplicas": 4,
  "workers": 1,
  "routes": {
    "createDocument": {
      "path": "/_v/master-data/:dataEntity/create",
      "public": true
    },
    "getDocument": {
      "path": "/_v/master-data/:dataEntity/get/:id",
      "public": true
    },
    "updateDocument": {
      "path": "/_v/master-data/:dataEntity/update/:id",
      "public": true
    },
    "deleteDocument": {
      "path": "/_v/master-data/:dataEntity/delete/:id",
      "public": true
    },
    "listDocuments": {
      "path": "/_v/master-data/:dataEntity/search",
      "public": true
    }
  }
}
